<query id="insertMemberBanLog" action="insert">
	<tables>
		<table name="userblock_log" />
	</tables>
	<columns>
		<column name="id" var="id" />
		<column name="member_srl" var="member_srl" />
		<column name="nick_name" var="nick_name" />
		<column name="ipaddress" var="ipaddress" />

		<column name="action" var="action" />

		<column name="target_member_srl" default="0" var="target_member_srl" />
		<column name="target_nick_name" var="target_nick_name" />
		<column name="target_ipaddress" var="target_ipaddress" />
		<column name="target_srl" var="target_srl" />

		<column name="day" var="day" />
		<column name="hour" var="hour" />
		<column name="type" var="type" />
		<column name="use_ipban" default="N" var="use_ipban" />

		<column name="expdate" var="expdate" />
		<column name="regdate" var="regdate" />
		<column name="comment" var="comment" />
	</columns>
</query>
